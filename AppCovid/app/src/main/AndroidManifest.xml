<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aplicacion">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">

        <activity android:name=".ui.registrar.registroPersona">

        </activity>

        <activity android:name=".ui.registrar.RegistrarActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <!-- triaje -->
        <activity
            android:name=".TriajeActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.test.Test1Activity"
            android:parentActivityName=".TriajeActivity"/>
        <activity
            android:name=".ui.test.Test2Activity"
            android:parentActivityName=".ui.test.Test1Activity" />
        <activity
            android:name=".ui.test.Test3Activity"
            android:parentActivityName=".ui.test.Test2Activity" />
        <activity
            android:name=".ui.test.Test4Activity"
            android:parentActivityName=".MainActivity"/>

    </application>

</manifest>